{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fs-blocks/carousel-slide",
  "title": "Carousel Slide",
  "category": "design",
  "parent": [
    "fs-blocks/carousel"
  ],
  "description": "A single slide inside the carousel.",
  "icon": "slides",
  "supports": {
    "html": false,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": false,
        "width": true
      }
    },
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "link": false
      }
    },
    "spacing": {
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    }
  },
  "attributes": {
    "verticalAlign": {
      "type": "string"
    }
  },
  "textdomain": "fancy-squares-core-enhancements",
  "render": "file:./render.php"
}