{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fs-blocks/index-block",
  "title": "Index Block",
  "category": "layout",
  "parent": [
    "core/column"
  ],
  "supports": {
    "html": false,
    "anchor": true,
    "color": {
      "text": true,
      "background": true,
      "gradients": true
    }
  },
  "attributes": {
    "blockIndex": {
      "type": "number",
      "default": -1
    },
    "additionalClasses": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    }
  },
  "textdomain": "fancy-squares-core-enhancements",
  "render": "file:./render.php"
}